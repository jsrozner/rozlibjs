{
  "name": "@rozlib/eslint_eslint-config-collection",
  "version": "0.0.1",
  "description": "Shared eslint configurations",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "MIT",
  "author": "Matthew Zito <exbotanical@protonmail.com>",
  "main": "index.js",
  "private": false,
  "scripts": {
    "format": "prettier . --write --ignore-path node_modules/@rozlib/prettier-config/.prettierignore",
    "lint": "eslint . --fix",
    "prep": "pnpm lint && pnpm format && git add .",
    "release": "lerna publish --conventional-commits"
  },
  "devDependencies": {
    "@rozlib/eslint-config": "*",
    "@rozlib/prettier-config": "^0.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "8.6.0",
    "lerna": "^4.0.0",
    "prettier": "2.5.0",
    "typescript": "^4.7"
  },
  "prettier": "@rozlib/prettier-config"
}
